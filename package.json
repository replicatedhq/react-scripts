{
  "name": "@replicatedhq/react-scripts",
  "version": "1.0.0",
  "description": "Replicated's fork of Create React App Typescript for shared configuration between projects.",
  "repository": "replicatedhq/react-scripts",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "bugs": {
    "url": "https://github.com/replicatedhq/react-scripts/issues"
  },
  "files": [
    "bin",
    "config",
    "scripts",
    "template",
    "utils"
  ],
  "scripts": {
    "format": "npm-run-all eslint-fix tslint-fix prettier",
    "lint": "npm-run-all eslint tslint prettier-lint",
    "eslint": "eslint .",
    "eslint-fix": "npm run eslint -- --fix",
    "tslint": "tslint --project template -c tslint.json",
    "tslint-fix": "npm run tslint -- --fix",
    "prettier": "prettier --write -c template/.prettierrc template/**/*.ts template/**/*.tsx",
    "prettier-lint": "prettier-check -c template/.prettierrc template/**/*.ts template/**/*.tsx"
  },
  "bin": {
    "react-scripts-replicated": "./bin/react-scripts-ts.js"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-emotion": "^9.2.8",
    "babel-preset-react-app": "^3.1.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "monaco-editor-webpack-plugin": "^1.5.2",
    "node-sass": "^4.9.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "^5.0.2",
    "resolve": "1.6.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.11.0",
    "tslint-config-replicated": "^1.0.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "eslint": "^5.5.0",
    "monaco-editor": "^0.14.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "prettier-check": "^2.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "typescript": "3.0.3"
  },
  "peerDependencies": {
    "typescript": "3.x.x"
  },
  "optionalDependencies": {
    "fsevents": "^1.1.3"
  }
}
